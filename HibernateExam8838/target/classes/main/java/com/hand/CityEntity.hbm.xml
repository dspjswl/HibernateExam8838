<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hand.CityEntity" table="city" schema="" catalog="sakila">
        <id name="cityId">
            <column name="city_id" sql-type="smallint unsigned" not-null="true"/>
        </id>
        <property name="city">
            <column name="city" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>